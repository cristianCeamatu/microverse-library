{
  "name": "template-eslint-stylelint",
  "version": "1.0.0",
  "description": "node_modules/",
  "main": "index.js",
  "scripts": {
    "eslint": "eslint .",
    "stylelint": "stylelint \"**/*.{css,scss}\"",
    "fix": "stylelint --fix \"**/*.{css,scss}\" && eslint . --fix",
    "test": "npm run fix && npm run stylelint && npm run eslint && echo '🎉 All test pass 🎉'",
    "watch": "node-sass -w assets/scss/ -o assets/css/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cristianCeamatu/template-eslint-stylelint.git"
  },
  "keywords": [],
